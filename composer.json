{
  "name": "spryker/search-elasticsearch",
  "description": "SearchElasticsearch module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/elastica": "^3.0.0",
    "spryker/log": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/search-extension": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/zend": "^3.0.0",
    "spryker/util-encoding": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
